{
  "name": "@lrnwebcomponents/app-hax",
  "wcfactory": {
    "className": "AppHax",
    "customElementClass": "LitElement",
    "elementName": "app-hax",
    "generator-wcfactory-version": "0.13.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/app-hax.css",
      "scss": "src/app-hax.scss",
      "html": "src/app-hax.html",
      "js": "src/app-hax.js",
      "properties": "src/app-hax-properties.json",
      "hax": "src/app-hax-hax.json"
    },
    "iconset": {},
    "sharedHaxProps": [],
    "sharedProps": [],
    "sharedStyles": []
