{
  "name": "@lrnwebcomponents/learn-two-theme",
  "wcfactory": {
    "className": "LearnTwoTheme",
    "customElementClass": "LitElement",
    "elementName": "learn-two-theme",
    "generator-wcfactory-version": "0.7.0",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/learn-two-theme.css",
      "scss": "src/learn-two-theme.scss",
      "html": "src/learn-two-theme.html",
      "js": "src/learn-two-theme.js",
      "properties": "src/learn-two-theme-properties.json",
      "hax": "src/learn-two-theme-hax.json"
    },
    "sharedStyles": []
  },
  "version": "3.0.4",
  "description": "Learn2 theme for HAXcms",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "learn-two-theme.js",
  "module": "learn-two-theme.js",
  "umd": "learn-two-theme.umd.js",
  "scripts": {
    "test": "wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/learn-two-theme/test/",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && rollup -c && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/hax-iconset": "^3.0.0",
    "@lrnwebcomponents/haxcms-elements": "^3.0.4",
    "@lrnwebcomponents/map-menu": "^3.0.3",
    "@lrnwebcomponents/simple-colors": "^3.0.0",
    "@lrnwebcomponents/simple-icon": "^3.0.0",
    "@lrnwebcomponents/simple-tooltip": "^3.0.0",
    "@polymer/app-layout": "^3.0.2",
    "@polymer/polymer": "^3.3.1"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^3.0.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "concurrently": "5.3.0",
    "gulp-babel": "8.0.0",
    "lodash": "4.17.19",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
