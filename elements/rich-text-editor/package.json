{
  "name": "@lrnwebcomponents/rich-text-editor",
  "wcfactory": {
    "className": "RichTextEditor",
    "customElementClass": "LitElement",
    "elementName": "rich-text-editor",
    "generator-wcfactory-version": "0.6.5",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/rich-text-editor.css",
      "scss": "src/rich-text-editor.scss",
      "html": "src/rich-text-editor.html",
      "js": "src/rich-text-editor.js",
      "properties": "src/rich-text-editor-properties.json",
      "hax": "src/rich-text-editor-hax.json"
    },
    "sharedStyles": [
      "...RichTextStyles"
    ]
  },
  "version": "4.1.0",
  "description": "a standalone rich text editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "rich-text-editor.js",
  "module": "rich-text-editor.js",
  "umd": "rich-text-editor.umd.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && node --experimental-json-modules ../../node_modules/.bin/rollup --config && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/absolute-position-behavior": "^4.1.0",
    "@lrnwebcomponents/code-editor": "^4.1.0",
    "@lrnwebcomponents/md-extra-icons": "^4.1.0",
    "@lrnwebcomponents/simple-fields": "^4.1.0",
    "@lrnwebcomponents/simple-icon": "^4.1.0",
    "@lrnwebcomponents/simple-icon-picker": "^4.1.0",
    "@lrnwebcomponents/simple-picker": "^4.1.0",
    "@lrnwebcomponents/simple-popover": "^4.1.0",
    "@lrnwebcomponents/simple-toolbar": "^4.1.0",
    "@lrnwebcomponents/utils": "^4.1.0",
    "@polymer/iron-icons": "^3.0.1",
    "lit": "^2.3.1",
    "shadow-selection-polyfill": "^1.1.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^4.0.0",
    "@lrnwebcomponents/storybook-utilities": "^4.1.0",
    "@open-wc/testing": "3.0.4",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.1.34",
    "@webcomponents/webcomponentsjs": "2.6.0",
    "concurrently": "7.4.0",
    "gulp-babel": "8.0.0",
    "lodash": "^4.17.21",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ],
  "gitHead": "fa72bd71c8e0e3c06d28d6ba849c39e86741f34d"
}
