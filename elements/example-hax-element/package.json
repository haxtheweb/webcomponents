{
  "name": "@lrnwebcomponents/example-hax-element",
  "wcfactory": {
    "className": "ExampleHaxElement",
    "customElementClass": "HTMLElement",
    "elementName": "example-hax-element",
    "generator-wcfactory-version": "0.5.0",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/example-hax-element.css",
      "scss": "src/example-hax-element.scss",
      "html": "src/example-hax-element.html",
      "js": "src/example-hax-element.js",
      "properties": "src/example-hax-element-properties.json",
      "hax": "src/example-hax-element-hax.json"
    }
  },
  "version": "0.0.34",
  "description": "Provide an example to pick apart of a working HAX element",
  "repository": {
    "type": "git",
    "url": "git@github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "example-hax-element.js",
  "module": "example-hax-element.es6.js",
  "umd": "example-hax-element.umd.js",
  "amd": "example-hax-element.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/example-hax-element/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "open ./src && concurrently --kill-others 'yarn run watch' 'yarn run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/hax-body-behaviors": "^0.0.34"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^0.0.31",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.0.2",
    "@webcomponents/webcomponentsjs": "2.1.3",
    "concurrently": "4.0.1",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.4",
    "lodash": "4.17.11",
    "polymer-build": "3.0.4",
    "polymer-cli": "1.8.0",
    "wct-browser-legacy": "1.0.1",
    "web-animations-js": "^2.3.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
