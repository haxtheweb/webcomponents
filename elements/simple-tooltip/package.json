{
  "name": "@lrnwebcomponents/simple-tooltip",
  "wcfactory": {
    "className": "SimpleTooltip",
    "customElementClass": "LitElement",
    "elementName": "simple-tooltip",
    "generator-wcfactory-version": "0.7.4",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/simple-tooltip.css",
      "scss": "src/simple-tooltip.scss",
      "html": "src/simple-tooltip.html",
      "js": "src/simple-tooltip.js",
      "properties": "src/simple-tooltip-properties.json",
      "hax": "src/simple-tooltip-hax.json"
    }
  },
  "version": "2.7.10",
  "description": "a simple tooltip forked from paper-tooltip with the same api",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "simple-tooltip.js",
  "module": "simple-tooltip.js",
  "umd": "simple-tooltip.umd.js",
  "scripts": {
    "test": "wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/simple-tooltip/test/",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && rollup -c && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lit-element": "2.4.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.7.7",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.4.4",
    "concurrently": "5.3.0",
    "gulp-babel": "8.0.0",
    "lodash": "4.17.19",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
