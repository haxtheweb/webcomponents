{
  "name": "@lrnwebcomponents/count-up",
  "wcfactory": {
    "className": "CountUpElement",
    "customElementClass": "LitElement",
    "elementName": "count-up",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/count-up.css",
      "scss": "src/count-up.scss",
      "html": "src/count-up.html",
      "js": "src/count-up.js",
      "properties": "src/count-up-properties.json",
      "hax": "src/count-up-hax.json"
    }
  },
  "version": "2.3.2",
  "description": "count up js wrapper with minimal styling",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "count-up.js",
  "module": "count-up.es6.js",
  "umd": "count-up.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/count-up/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}' && wca analyze \"**/*.js\" --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@polymer/polymer": "^3.2.0",
    "countup.js": "^2.0.4"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.1.1",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.4",
    "lodash": "4.17.13",
    "polymer-cli": "1.9.9",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
