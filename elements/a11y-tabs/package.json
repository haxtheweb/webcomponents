{
  "name": "@lrnwebcomponents/a11y-tabs",
  "wcfactory": {
    "className": "A11yTabs",
    "customElementClass": "LitElement",
    "elementName": "a11y-tabs",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/a11y-tabs.css",
      "scss": "src/a11y-tabs.scss",
      "html": "src/a11y-tabs.html",
      "js": "src/a11y-tabs.js",
      "properties": "src/a11y-tabs-properties.json",
      "hax": "src/a11y-tabs-hax.json"
    }
  },
  "version": "2.1.5",
  "description": "accessible and responsive tabbed interface",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "a11y-tabs.js",
  "module": "a11y-tabs.es6.js",
  "umd": "a11y-tabs.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/a11y-tabs/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/responsive-utility": "^2.1.4",
    "@polymer/iron-a11y-keys": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "lit-element": "2.2.1"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "2.0.5",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.11",
    "polymer-build": "3.1.2",
    "polymer-cli": "1.9.7",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
