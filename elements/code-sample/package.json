{
  "name": "@lrnwebcomponents/code-sample",
  "wcfactory": {
    "className": "CodeSample",
    "customElementClass": "LitElement",
    "elementName": "code-sample",
    "generator-wcfactory-version": "0.7.0",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/code-sample.css",
      "scss": "src/code-sample.scss",
      "html": "src/code-sample.html",
      "js": "src/code-sample.js",
      "properties": "src/code-sample-properties.json",
      "hax": "src/code-sample-hax.json"
    },
    "sharedStyles": []
  },
  "version": "7.0.1",
  "description": "A wrapper element for highlight.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "code-sample.js",
  "module": "code-sample.js",
  "umd": "code-sample.umd.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && node --experimental-json-modules ../../node_modules/.bin/rollup --config && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"{*,lib/**}.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lit": "^2.7.4"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^7.0.0",
    "@lrnwebcomponents/storybook-utilities": "^7.0.1",
    "@open-wc/testing": "3.0.4",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.1.34",
    "@webcomponents/webcomponentsjs": "2.6.0",
    "concurrently": "7.4.0",
    "gulp-babel": "8.0.0",
    "lodash": "^4.17.21",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html",
    "haxtheweb"
  ],
  "gitHead": "7087ebc5fd7f631a2a78ad40af08384933139929"
}
