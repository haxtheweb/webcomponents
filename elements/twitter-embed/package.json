{
  "name": "@lrnwebcomponents/twitter-embed",
  "wcfactory": {
    "className": "TwitterEmbed",
    "customElementClass": "HTMLElement",
    "elementName": "twitter-embed",
    "generator-wcfactory-version": "0.8.6",
    "useHAX": false,
    "useSass": false,
    "files": {
      "js": "src/twitter-embed.js"
    },
    "sharedHaxProps": [],
    "sharedProps": [],
