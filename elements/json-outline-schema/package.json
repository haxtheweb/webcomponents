{
  "name": "@lrnwebcomponents/json-outline-schema",
  "wcfactory": {
    "className": "JsonOutlineSchema",
    "customElementClass": "HTMLElement",
    "elementName": "json-outline-schema",
    "generator-wcfactory-version": "0.6.3",
    "useHAX": false,
    "useSass": false,
    "files": {
      "js": "src/json-outline-schema.js"
    },
    "sharedStyles": []
  },
  "version": "2.7.7",
  "description": "JS based state management helper for the json outline schema spec",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "json-outline-schema.js",
  "module": "json-outline-schema.js",
  "umd": "json-outline-schema.umd.js",
  "scripts": {
    "test": "wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/json-outline-schema/test/",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && rollup -c && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.7.7",
    "@lrnwebcomponents/storybook-utilities": "^2.7.7",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.4.1",
    "concurrently": "5.1.0",
    "gulp-babel": "8.0.0",
    "lodash": "4.17.19",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
