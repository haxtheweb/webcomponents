{
  "name": "@lrnwebcomponents/r-coder",
  "wcfactory": {
    "className": "RCoder",
    "customElementClass": "LitElement",
    "elementName": "r-coder",
    "generator-wcfactory-version": "0.7.3",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/r-coder.css",
      "scss": "src/r-coder.scss",
      "html": "src/r-coder.html",
      "js": "src/r-coder.js",
      "properties": "src/r-coder-properties.json",
      "hax": "src/r-coder-hax.json"
    }
  },
  "version": "2.3.7",
  "description": "R coder interface for the r-service backend.",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "r-coder.js",
  "module": "r-coder.es6.js",
  "umd": "r-coder.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/r-coder/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}' && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others 'yarn run watch' 'yarn run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "heymp"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/code-editor": "^2.3.7",
    "@polymer/paper-button": "^3.0.0",
    "@polymer/polymer": "^3.3.1"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "concurrently": "4.0.1",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.4",
    "lodash": "4.17.13",
    "polymer-build": "3.1.0",
    "polymer-cli": "1.8.1",
    "wct-browser-legacy": "1.0.2"
  },
  "private": true,
  "keywords": [
    "web-components",
    "html"
  ]
}
