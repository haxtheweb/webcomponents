{
  "name": "@lrnwebcomponents/simple-modal",
  "wcfactory": {
    "className": "SimpleModal",
    "customElementClass": "LitElement",
    "elementName": "simple-modal",
    "generator-wcfactory-version": "0.4.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/simple-modal.css",
      "scss": "src/simple-modal.scss",
      "html": "src/simple-modal.html",
      "js": "src/simple-modal.js",
      "properties": "src/simple-modal-properties.json",
      "hax": "src/simple-modal-hax.json"
    },
    "sharedStyles": []
  },
  "version": "4.0.24",
  "description": "A simple modal that ensures accessibility and stack order context appropriately",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "simple-modal.js",
  "module": "simple-modal.js",
  "umd": "simple-modal.umd.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && node --experimental-json-modules ../../node_modules/.bin/rollup --config && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium firefox"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/simple-icon": "^4.0.24",
    "lit": "^2.2.5",
    "web-dialog": "0.0.11"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^4.0.0",
    "@lrnwebcomponents/simple-fields": "^4.0.24",
    "@lrnwebcomponents/storybook-utilities": "^4.0.24",
    "@open-wc/testing": "3.0.4",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.1.31",
    "@webcomponents/webcomponentsjs": "2.6.0",
    "concurrently": "7.0.0",
    "gulp-babel": "8.0.0",
    "lodash": "^4.17.21",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ],
  "gitHead": "dfb2cd70d6c85d3ac0792679b3120c20923d5ada"
}
