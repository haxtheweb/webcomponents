    "elementName": "lrnsys-comment",
    "generator-wcfactory-version": "0.3.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/lrnsys-comment.css",
      "scss": "src/lrnsys-comment.scss",
      "html": "src/lrnsys-comment.html",
      "js": "src/lrnsys-comment.js",
