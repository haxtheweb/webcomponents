{
  "name": "@lrnwebcomponents/elmsln-studio",
  "wcfactory": {
    "className": "ElmslnStudio",
    "customElementClass": "LitElement",
    "elementName": "elmsln-studio",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "js": "src/elmsln-studio.js"
    }
  },
  "version": "3.0.0",
  "description": "Studio App for ELMS:LN",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "elmsln-studio.js",
  "module": "elmsln-studio.js",
  "umd": "elmsln-studio.umd.js",
  "scripts": {
    "test": "wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/elmsln-studio/test/",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && rollup -c && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\"",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/a11y-collapse": "^3.0.0",
    "@lrnwebcomponents/accent-card": "^3.0.0",
    "@lrnwebcomponents/hax-iconset": "^3.0.0",
    "@lrnwebcomponents/img-view-modal": "^3.0.0",
    "@lrnwebcomponents/lrndesign-avatar": "^3.0.0",
    "@lrnwebcomponents/lrndesign-gallery": "^3.0.0",
    "@lrnwebcomponents/nav-card": "^3.0.0",
    "@lrnwebcomponents/simple-colors": "^3.0.0",
    "@lrnwebcomponents/simple-fields": "^3.0.0",
    "@lrnwebcomponents/simple-icon": "^3.0.0",
    "@lrnwebcomponents/simple-modal": "^3.0.0",
    "lit-element": "2.4.0",
    "lit-element-router": "^2.0.3"
  },
  "devDependencies": {
    "@github/time-elements": "3.1.1",
    "@lrnwebcomponents/deduping-fix": "^3.0.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "concurrently": "5.3.0",
    "gulp-babel": "8.0.0",
    "lodash": "4.17.19",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
