{
  "name": "@lrnwebcomponents/voice-recorder",
  "wcfactory": {
    "className": "VoiceRecorder",
    "customElementClass": "LitElement",
    "elementName": "voice-recorder",
    "generator-wcfactory-version": "0.7.4",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/voice-recorder.css",
      "scss": "src/voice-recorder.scss",
      "html": "src/voice-recorder.html",
      "js": "src/voice-recorder.js",
      "properties": "src/voice-recorder-properties.json",
      "hax": "src/voice-recorder-hax.json"
    }
  },
  "version": "3.0.0",
  "description": "LAME bridge",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "voice-recorder.js",
  "module": "voice-recorder.js",
  "umd": "voice-recorder.umd.js",
  "scripts": {
    "test": "wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/voice-recorder/test/",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && rollup -c && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/hax-iconset": "^3.0.0",
    "@lrnwebcomponents/simple-icon": "^3.0.0",
    "lit-element": "2.4.0",
    "vmsg": "0.3.6"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^3.0.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.5.0",
    "concurrently": "5.3.0",
    "gulp-babel": "8.0.0",
    "lodash": "4.17.19",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
