{
  "name": "mammoth",
  "version": "1.4.18",
  "author": "Michael Williamson <mike@zwobble.org>",
  "description": "Convert Word documents from docx to simple HTML and Markdown",
  "keywords": [
    "docx",
    "html",
    "office",
    "word",
    "markdown",
    "md"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mwilliamson/mammoth.js.git"
  },
  "dependencies": {
    "argparse": "~2.0.0",
    "bluebird": "~3.7.0",
    "dingbat-to-unicode": "^1.0.1",
    "jszip": "^3.7.1",
    "lop": "^0.4.1",
    "path-is-absolute": "^1.0.0",
    "sax": "~1.2.0",
    "underscore": "^1.13.1",
    "xmlbuilder": "^15.0.0"
  },
  "devDependencies": {
    "browserify": "~17.0.0",
    "browserify-prepend-licenses": "~1.0.0",
    "duck": "^0.1.12",
    "eslint": "8.2.0",
    "hamjest": "3.7.3",
    "mocha": "~9.1.0",
    "temp": "~0.9.0",
    "uglify-js": "~3.14.0"
  },
  "browser": {
    "./lib/unzip.js": "./browser/unzip.js",
    "./lib/docx/files.js": "./browser/docx/files.js"
  },
  "bin": {
    "mammoth": "bin/mammoth"
