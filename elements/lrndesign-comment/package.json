{
  "name": "@lrnwebcomponents/lrndesign-comment",
  "wcfactory": {
    "className": "LrndesignComment",
    "customElementClass": "PolymerElement",
    "elementName": "lrndesign-comment",
    "generator-wcfactory-version": "0.3.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/lrndesign-comment.css",
      "scss": "src/lrndesign-comment.scss",
      "html": "src/lrndesign-comment.html",
      "js": "src/lrndesign-comment.js",
      "properties": "src/lrndesign-comment-properties.json",
      "hax": "src/lrndesign-comment-hax.json"
    }
  },
  "version": "0.0.26",
  "description": "Automated conversion of lrndesign-comment/",
  "repository": {
    "type": "git",
    "url": "git@github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "lrndesign-comment.js",
  "module": "lrndesign-comment.es6.js",
  "umd": "lrndesign-comment.umd.js",
  "amd": "lrndesign-comment.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/lrndesign-comment/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "open ./src && concurrently --kill-others 'yarn run watch' 'yarn run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/hax-body-behaviors": "^0.0.20",
    "@polymer/polymer": "^3.0.5"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.0.2",
    "@webcomponents/webcomponentsjs": "2.1.3",
    "concurrently": "4.0.1",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.4",
    "lodash": "4.17.11",
    "polymer-build": "3.0.4",
    "polymer-cli": "1.8.0",
    "wct-browser-legacy": "1.0.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
