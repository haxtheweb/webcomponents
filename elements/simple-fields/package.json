{
  "name": "@lrnwebcomponents/simple-fields",
  "wcfactory": {
    "className": "SimpleFields",
    "customElementClass": "LitElement",
    "elementName": "simple-fields",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/simple-fields.css",
      "scss": "src/simple-fields.scss",
      "html": "src/simple-fields.html",
      "js": "src/simple-fields.js",
      "properties": "src/simple-fields-properties.json",
      "hax": "src/simple-fields-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "2.6.4",
  "description": "Uses JSON Schema or an array of fields to display a series of fields",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "simple-fields.js",
  "module": "simple-fields.es6.js",
  "umd": "simple-fields.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/simple-fields/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}' && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "../../node_modules/.bin/concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "../../node_modules/.bin/es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/a11y-tabs": "^2.6.4",
    "@lrnwebcomponents/code-editor": "^2.6.4",
    "@lrnwebcomponents/hax-body": "^2.6.4",
    "@lrnwebcomponents/simple-colors": "^2.6.4",
    "@lrnwebcomponents/simple-icon-picker": "^2.6.4",
    "@lrnwebcomponents/simple-picker": "^2.6.3",
    "@lrnwebcomponents/simple-tooltip": "^2.6.4",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/marked-element": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-icon-button": "^3.0.1",
    "lit-element": "2.2.1"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.4.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.4.1",
    "concurrently": "5.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.13",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
