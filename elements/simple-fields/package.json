{
  "name": "@haxtheweb/simple-fields",
  "wcfactory": {
    "className": "SimpleFields",
    "customElementClass": "LitElement",
    "elementName": "simple-fields",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/simple-fields.css",
      "html": "src/simple-fields.html",
      "js": "src/simple-fields.js",
      "properties": "src/simple-fields-properties.json",
      "hax": "src/simple-fields-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "9.0.9",
  "description": "Uses JSON Schema or an array of fields to display a series of fields",
  "repository": {
    "type": "git",
    "url": "https://github.com/haxtheweb/webcomponents.git"
  },
  "type": "module",
  "main": "simple-fields.js",
  "module": "simple-fields.js",
  "scripts": {
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"{*,lib/**}.js\" --format vscode --outFile custom-elements.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "web-dev-server -c ../../web-dev-server.config.mjs",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs",
    "test:watch": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium --watch",
    "test": "web-test-runner \"test/**/*.test.js\" --node-resolve --config=../../web-test-runner.config.mjs --playwright --browsers chromium"
  },
  "author": {
    "name": "haxtheweb",
    "url": "https://hax.psu.edu/"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@haxtheweb/a11y-tabs": "^9.0.7",
    "@haxtheweb/code-editor": "^9.0.9",
    "@haxtheweb/hax-iconset": "^9.0.7",
    "@haxtheweb/i18n-manager": "^9.0.7",
    "@haxtheweb/responsive-utility": "^9.0.7",
    "@haxtheweb/simple-colors": "^9.0.7",
    "@haxtheweb/simple-icon": "^9.0.7",
    "@haxtheweb/simple-icon-picker": "^9.0.7",
    "@haxtheweb/simple-login": "^9.0.7",
    "@haxtheweb/simple-picker": "^9.0.7",
    "@haxtheweb/simple-toolbar": "^9.0.7",
    "@haxtheweb/simple-tooltip": "^9.0.7",
    "@haxtheweb/utils": "^9.0.7",
    "@haxtheweb/voice-recorder": "^9.0.7",
    "@vaadin/vaadin-upload": "^4.4.1",
    "lit": "^3.2.0"
  },
  "devDependencies": {
    "@haxtheweb/deduping-fix": "^9.0.7",
    "@open-wc/testing": "4.0.0",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@web/dev-server": "0.4.6",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "concurrently": "8.2.2",
    "wct-browser-legacy": "1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "customElements": "custom-elements.json",
  "keywords": [
    "webcomponents",
    "html",
    "haxtheweb",
    "lit"
  ],
  "gitHead": "7f62cde6a6242afcb57715e1e2067ea767c777fd"
}
