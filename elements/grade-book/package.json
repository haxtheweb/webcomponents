    "className": "GradeBook",
    "customElementClass": "LitElement",
    "elementName": "grade-book",
    "generator-wcfactory-version": "0.11.0",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/grade-book.css",
      "html": "src/grade-book.html",
      "js": "src/grade-book.js",
