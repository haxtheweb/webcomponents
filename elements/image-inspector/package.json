{
  "name": "@lrnwebcomponents/image-inspector",
  "wcfactory": {
    "className": "ImageInspector",
    "customElementClass": "PolymerElement",
    "elementName": "image-inspector",
    "generator-wcfactory-version": "0.3.1",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/image-inspector.css",
      "scss": "src/image-inspector.scss",
      "html": "src/image-inspector.html",
      "js": "src/image-inspector.js",
      "properties": "src/image-inspector-properties.json",
      "hax": "src/image-inspector-hax.json"
    }
  },
  "version": "2.1.5",
  "description": "Inspector tools on an image wrapping open seadragon library",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "image-inspector.js",
  "module": "image-inspector.es6.js",
  "umd": "image-inspector.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/image-inspector/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
