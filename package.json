{
  "name": "@haxtheweb/webcomponents",
  "description": "HAXTheWeb web component library",
  "orgNpm": "@haxtheweb",
  "monorepo": "webcomponents",
  "wcfactory": {
    "askSASS": false,
    "askProps": false,
    "askHAX": false,
    "monorepo": "webcomponents",
    "orgNpm": "@haxtheweb",
    "orgGit": "haxtheweb",
    "gitRepo": "https://github.com/haxtheweb/webcomponents.git",
    "sass": {
      "name": "rh-sass",
      "pkg": "@rhelements/rh-sass",
      "path": "rh-sass/rh-sass"
