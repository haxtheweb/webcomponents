{
  "name": "lrnwebcomponents",
  "description": "ELMSLN produced web components for any project",
  "wcfactory": {
    "askSASS": true,
    "askProps": true,
    "askHAX": true,
    "monorepo": "lrnwebcomponents",
    "orgNpm": "@lrnwebcomponents",
    "orgGit": "elmsln",
    "gitRepo": "git@github.com/elmsln/lrnwebcomponents.git",
    "sass": {
      "name": "rh-sass",
      "pkg": "@rhelements/rh-sass",
      "path": "rh-sass/rh-sass"
    }
  },
  "author": {
    "name": "elmsln",
    "url": "https://www.elmsln.org/"
  },
  "license": "Apache-2.0",
  "repository": "git@github.com/elmsln/lrnwebcomponents.git",
  "scripts": {
    "start": "yarn run storybook",
    "lerna": "lerna",
    "init": "lerna init",
    "postinstall": "lerna link",
    "test": "yarn run build && wct --configFile wct.conf.json test/index.html",
    "build": "lerna run build",
    "publish": "echo 'NOTICE: You must run `lerna publish` manually'",
    "test-suite-inject": "node scripts/test-suite-inject.js",
    "storybook": "start-storybook -c .storybook -p 9001 -s ./elements",
    "build-storybook": "build-storybook -c .storybook -o ../../storybooks/lrnwebcomponents",
    "rebuild-wcfcache": "./node_modules/.bin/gulp",
    "new": "yarn run rebuild-wcfcache && wcf element && yarn run test-suite-inject && yarn install --link-duplicates"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@emotion/core": "^10.0.0-beta.8",
    "@emotion/styled-base": "^0.10.6",
    "@storybook/addon-a11y": "4.0.2",
    "@storybook/addon-actions": "4.0.2",
    "@storybook/addon-knobs": "4.0.2",
    "@storybook/addon-notes": "4.0.2",
    "@storybook/addon-options": "4.0.2",
    "@storybook/addon-storysource": "4.0.2",
    "@storybook/addon-viewport": "4.0.2",
    "@storybook/addons": "4.0.2",
    "@storybook/cli": "4.0.2",
    "@storybook/polymer": "4.0.2",
    "@webcomponents/webcomponentsjs": "2.1.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-custom-element-classes": "0.1.0",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-preset-env": "1.7.0",
    "browser-sync": "2.26.3",
    "chalk": "^2.4.1",
    "css-scss": "0.1.1",
    "decomment": "0.9.2",
    "del": "3.0.0",
    "dialog-polyfill": "0.4.10",
    "global": "4.3.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "8.0.0",
    "gulp-clean-css": "3.10.0",
    "gulp-concat-util": "0.5.5",
    "gulp-copy": "4.0.1",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sass": "4.0.2",
    "gulp-sequence": "1.0.0",
    "gulp-shell": "0.6.5",
    "gulp-strip-css-comments": "2.0.0",
    "gulp-svg-sprite": "1.5.0",
    "gulp-trim": "1.0.2",
    "husky": "1.1.3",
    "inquirer-recursive": "0.0.5",
    "jest": "23.6.0",
    "lerna": "^3.4.1",
    "lint-staged": "8.0.4",
    "lodash": "4.17.11",
    "lodash.clonedeep": "4.5.0",
    "node-sass": "4.10.0",
    "polymer-build": "3.1.0",
    "polymer-webpack-loader": "2.0.3",
    "prettier": "1.14.3",
    "raw-loader": "0.5.1",
    "react": "*",
    "react-dom": "*",
    "requirejs": "2.3.6",
    "rollup": "0.67.0",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-terser": "3.0.0",
    "strip-css-comments": "3.0.0",
    "trim": "0.0.1",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.0",
    "webpack": "4.25.0",
    "whatwg-fetch": "3.0.0",
    "yo": "2.0.5"
  },
  "dependencies": {
    "@polymer/lit-element": "0.6.3",
    "@polymer/polymer": "3.1.0",
    "@skatejs/renderer-lit-html": "0.2.2",
    "lit-html": "0.13.0",
    "skatejs": "5.2.4",
    "slim-js": "4.0.5",
    "twig": "1.12.0"
  },
  "lint-staged": {
    "linters": {
      "*.{js,json}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "elements/**/*.*.js",
      "**/analysis-error.json",
      "themes/**/*.*.js",
      "libraries/**/*.js"
    ]
  },
  "keywords": [
    "yeoman-generator",
    "custom-elements",
    "web-components",
    "webcomponents",
    "polymer",
    "lit-element",
    "skate-js",
    "slim-js",
    "twig",
    "lit-html",
    "vanillajs"
  ],
  "private": true,
  "npmClient": "yarn",
  "useWorkspaces": true,
  "workspaces": {
    "packages": [
      "elements/*",
      "themes/*"
    ],
    "nohoist": [
      "**/@lrnwebcomponents",
      "**/@polymer",
      "**/@polymer/**",
      "**/@vaadin",
      "**/@vaadin/**",
      "**/@rhelements/rhelement",
      "**/@skatejs",
      "**/@skatejs/**",
      "**/skatejs",
      "**/@webcomponents",
      "**/@webcomponents/**",
      "**/lit-html",
      "**/lit-html/**",
      "**/paper-collapse-item",
      "**/paper-collapse-item/**",
      "**/slim-js",
      "**/slim-js/**",
      "**/jsoneditor",
      "**/jsoneditor/**",
      "**/ace-builds",
      "**/ace-builds/**",
      "**/juicy-ace-editor",
      "**/juicy-ace-editor/**",
      "**/moment",
      "**/moment/**",
      "**/twig",
      "**/twig/**",
      "**/jarallax",
      "**/jarallax/**",
      "**/screenfull",
      "**/screenfull/**",
      "**/pdfjs-dist",
      "**/pdfjs-dist/**",
      "**/web-animations-js",
      "**/web-animations-js/**",
      "**/chartist",
      "**/chartist/**",
      "**/ical.js",
      "**/ical.js/**",
      "**/time-elements",
      "**/time-elements/**",
      "**/@vowo",
      "**/@vowo/**",
      "**/wavesurfer.js",
      "**/wavesurfer.js/**",
      "**/aframe",
      "**/openseadragon",
      "**/mathjax"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test-suite-inject > /dev/null && lint-staged"
    }
  }
}
