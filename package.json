{
  "name": "@lrnwebcomponents/lrnwebcomponents",
  "scripts": {
    "start": "spandx",
    "lerna": "lerna",
    "init": "lerna init",
    "test": "npm run build && wct --configFile wct.conf.json test/index.html",
    "build": "lerna run build",
    "bootstrap": "lerna bootstrap --hoist",
    "postinstall": "npm run bootstrap",
    "precommit": "npm run test-suite-inject > /dev/null && lint-staged",
    "test-suite-inject": "node scripts/test-suite-inject.js",
    "storybook": "start-storybook -c .storybook -p 9001 -s ./elements",
    "build-storybook": "build-storybook -c .storybook -o .storybook_out",
    "new": "./node_modules/.bin/gulp && cd elements && wcfactory new && npm run test-suite-inject && npm run bootstrap"
  },
